        const supportsTemplates =function () {
            return 'content' in document.createElement('template');
        }
        const createCards = function (jsonData) {
            if (jsonData["volumeInfo"]) {
                createCard(jsonData);
            } else {
                if (jsonData["items"]) {
                    for (const x of jsonData["items"]) {
                        createCard(x);
                    }
                }
            }
        }
        const getBasicInfo = function (jsonData) {

            let infoData = new Object();
                infoData.description = jsonData["volumeInfo"]["description"];
                infoData.publisher = jsonData["volumeInfo"]["publisher"];
                infoData.pageCount = jsonData["volumeInfo"]["pageCount"];
                infoData.printedPageCount = jsonData["volumeInfo"]["printedPageCount"];
                infoData.printType = jsonData["volumeInfo"]["printType"];

            return infoData;
        }

        const getRatingInfo = function (jsonData) {
            let infoData = new Object();
            if (jsonData["volumeInfo"]["averageRating"]){
                infoData.averageRating = jsonData["volumeInfo"]["averageRating"];
            }
            if (jsonData["volumeInfo"]["ratingsCount"]){
                infoData.ratingsCount = jsonData["volumeInfo"]["ratingsCount"];
            }
            if (jsonData["volumeInfo"]["maturityRating"]){
                infoData.maturityRating = jsonData["volumeInfo"]["maturityRating"];
            }
            return infoData;
        }

        let looseObjects = new Object();
        const filter = ["title", "subtitle", "authors", "publishedDate", "description",
            "publisher", "pageCount", "printedPageCount", "printType", "averageRating",
            "ratingsCount", "maturityRating", "categories"
        ]

        const createCard = function (jsonData) {
            let tempCard = document.getElementById('card-template');
            let content = tempCard.content.cloneNode(true);
            content.getElementById('book-image').src = jsonData["volumeInfo"]["imageLinks"]["thumbnail"];
            content.getElementById('title').innerHTML = jsonData["volumeInfo"]["title"];
            if (jsonData["volumeInfo"]["subtitle"]) {
                content.getElementById('subtitle').innerHTML = jsonData["volumeInfo"]["subtitle"];
            }

            let authors = "";

            if (jsonData["volumeInfo"]["authors"].length > 1) {
                for (const x of jsonData["volumeInfo"]["authors"]) {
                    authors.concat(x+", ");
                }
            } else {
                authors = jsonData["volumeInfo"]["authors"][0];
            }

            content.getElementById('publish-date').innerHTML = jsonData["volumeInfo"]["publishedDate"]
            content.getElementById('author').innerHTML = content.getElementById('author').innerHTML.concat(authors);

            let container = document.createElement("div");
            container.className = "container";
            container.appendChild(createInfoBox(getBasicInfo(jsonData), "Basic Info"));
            content.getElementById("card-content").appendChild(container);

            container = document.createElement("div");
            container.className = "container";
            container.appendChild(createInfoBox(getRatingInfo(jsonData), "Rating Info"));

            let categories = new Object();
            categories.categories = jsonData["volumeInfo"]["categories"]
            container.appendChild(createInfoBox(categories, "Categories"));
            content.getElementById("card-content").appendChild(container);
            let count = 1;
            for (const x in jsonData){
                if (filter.includes(x)) {
                    continue
                }
                if (count % 2 != 0) {
                    container = document.createElement("div");
                    container.className = "container";
                    if (typeof jsonData[x] === 'string')
                    {
                        let test = new Object();
                        test[x] = [jsonData[x]];
                        container.appendChild(createInfoBox(test,x));
                    } else {
                        container.appendChild(createInfoBox(jsonData[x],x));
                    }

                } else {
                    if (typeof jsonData[x] === 'string')
                    {
                        let test = new Object();
                        test[x] = [jsonData[x]];
                        container.appendChild(createInfoBox(test,x));
                    } else {
                        container.appendChild(createInfoBox(jsonData[x],x));
                    }
                    content.getElementById("card-content").appendChild(container);
                }
                count++;
            }

            document.getElementById('content').appendChild(content);

        }

        const createInfoBox = function (jsonData, title, forceObjects) {
            let tempInfoBox = document.getElementById('info-box').content.cloneNode(true);
            tempInfoBox.getElementById('info-box-title').innerHTML = title;
            for (const x in jsonData) {
                if (typeof  jsonData[x] === 'string' || !isNaN(jsonData[x]))
                {
                    let newLine = document.createElement("p");
                    newLine.style.whiteSpace = "pre-wrap";
                    newLine.style.wordBreak = "break-all"
                    let newSpan = document.createElement("span");
                    newSpan.style.fontWeight = "bold";
                    newSpan.innerHTML = x.charAt(0).toUpperCase() + x.slice(1);
                    newLine.appendChild(newSpan)
                    newLine.innerHTML = newLine.innerHTML + " : " + jsonData[x];
                    tempInfoBox.getElementById('info-box-content').appendChild(newLine);
                }
                 if (Array.isArray(jsonData[x]) ) {

                     let newLine = document.createElement("p");
                     newLine.innerHTML = x.charAt(0).toUpperCase() + x.slice(1);
                     let newList = document.createElement("ul");
                     for (const v of jsonData[x] ){
                         let newItem = document.createElement("li");
                         if (typeof v === 'object'){
                             let newSubList = document.createElement("ul")
                             for (const y in v){
                                 let newSubItem = document.createElement("li");
                                 newSubItem.innerHTML = y;
                                 newSubList.appendChild(newSubItem);
                             }
                             newItem.innerHTML = v;
                             newItem.appendChild(newSubList);
                         } else {
                             newItem.innerHTML = v;
                         }

                         newList.appendChild(newItem);
                     }
                     tempInfoBox.getElementById('info-box-content').appendChild(newLine);
                     tempInfoBox.getElementById('info-box-content').appendChild(newList);
                 }
                 if (typeof jsonData[x] === 'object' && !Array.isArray(jsonData[x])) {
                     if (forceObjects) {
                         console.log("loose");
                     } else {
                         looseObjects[x] = jsonData[x];
                         console.log("loose");
                     }
                 }
            }

            return tempInfoBox;
        }

