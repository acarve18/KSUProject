        <template id="card-template">
            <div class="card">
                <div class="card-header container">
                    <img class="card-image" id="book-image"/>
                    <div class="card-title">
                        <h2 id="title"></h2>
                        <h3 id="subtitle"></h3>
                        <p id="author"><span style="font-weight: bold">By : </span> </p>
                        <p id="publish-date"><span style="font-weight: bold">Released : </span> </p>
                    </div>
